{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "experimentalObjectRestSpread": true
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "prefer-const": 2,
    "no-console": "warn"
  },
  "env": {
    "node": true,
    "jasmine": true
  }
}
